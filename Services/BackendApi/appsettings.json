{
  "Logging": {
    "FileLogger": {
      //"Path": "C:/temp/log.txt",
      "Use": false,
      "Append": false
    },
    "ColorConsole": {
      "LogLevelToColorMap": {
        "Information": "DarkGreen",
        "Warning": "Cyan",
        "Error": "Red",
        "Trace": "Gray",
        "Debug": "Blue"
      },
      "Use": true
    },
    "LogLevel": {
      "Default": "Trace",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "FileOptions": {
    "WaitTime": 1000,
    "Retires": 5
  },
  "AllowedHosts": "*",
  "Keycloak": {
    "Realm": "prism",
    "AuthServerUrl": "http://localhost:8180/",
    "SslRequired": "none",
    "Resource": "Backend",
    "PublicClient": true,
    "ConfidentialPort": 0,
    "VerifyTokenAudience": false
  },
  "AMQPOptions": {
    "Host": "localhost",
    "VirtualHost": "external",
    "Username": "backend",
    "Password": "backendTestPass1234$"
  }
}
